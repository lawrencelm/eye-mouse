{
  "manifest_version": 2,

  "name": "eye-mouse",
  "description": "Tracks a user's eye and maneuvers a virtual mouse pointer in the Chrome browser based on eye movement",
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": [
        "jquery.min.js",
        "build/jsfeat.js",
        "build/compatibility.js",
        "build/camgaze.js",
        "cascades/eye.js",
        "cascades/frontalface.js",
        "mouse-sim.js",
        "init.js"
      ]
    }
  ]

}

